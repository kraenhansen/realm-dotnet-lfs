{
    "metadata": [
        {
            "src": [
                {
                    "src": "../Realm.LFS",
                    "files": [
                        "Realm.LFS.csproj"
                    ],
                    "exclude": [
                        "**/obj/**",
                        "**/bin/**",
                        "_site/**"
                    ]
                },
                {
                    "src": "../Realm.LFS.S3",
                    "files": [
                        "Realm.LFS.S3.csproj"
                    ],
                    "exclude": [
                        "**/obj/**",
                        "**/bin/**",
                        "_site/**"
                    ]
                },
                {
                    "src": "../Realm.LFS.Functions",
                    "files": [
                        "Realm.LFS.Functions.csproj"
                    ],
                    "exclude": [
                        "**/obj/**",
                        "**/bin/**",
                        "_site/**"
                    ]
                },
                {
                    "src": "../Realm.LFS.Azure",
                    "files": [
                        "Realm.LFS.Azure.csproj"
                    ],
                    "exclude": [
                        "**/obj/**",
                        "**/bin/**",
                        "_site/**"
                    ]
                }
            ],
            "dest": "api",
            "filter": "filterConfig.yml",
            "disableGitFeatures": false,
            "disableDefaultFilter": false
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "articles/**.md",
                    "articles/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            },
            {
                "files": "CHANGELOG.md",
                "src": ".."
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "globalMetadata": {
            "_enableSearch": true,
            "_appTitle": "Realm LFS",
            "_appFaviconPath": "images/favicon.ico",
            "_appLogoPath": "images/logo.svg",
            "_appFooter": "Copyright Â© 2023 <a href=\"https://realm.io\">Realm</a><br>Generated by <strong>DocFX</strong>",
            "_gitContribute": {
                "repo": "https://github.com/realm/realm-dotnet-lfs.git",
                "branch": "main",
                "path": "Docs/apispec"
            }
        },
        "fileMetadataFiles": [],
        "template": [
            "default",
            "template"
        ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}